	<query language="sql"><![CDATA[SELECT
		PAYMENTS.PAYMENT,
		SUM( PAYMENTS.TOTAL ) AS PAYMENT_TOTAL
		FROM CLOSEDCASH, PAYMENTS, RECEIPTS
		WHERE CLOSEDCASH.MONEY = RECEIPTS.MONEY
		AND PAYMENTS.RECEIPT = RECEIPTS.ID
		GROUP BY PAYMENTS.PAYMENT]]></query>
